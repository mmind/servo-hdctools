<root>
  <!-- ******** Includes ******** -->
  <include>
    <name>x86_ec_common.xml</name>
  </include>
  <!-- ******** Controls ******** -->
  <control>
    <name>lid_open</name>
    <doc>emulated lid switch on ec</doc>
    <params subtype="lid_open" interface="7" drv="ec" map="yesno"
    clobber_ok=""></params>
  </control>
  <control>
    <name>power_state</name>
    <doc>Used to turn the DUT off and on</doc>
    <params cmd="set" interface="servo" drv="link_power" input_type="str"
      reset_hold="0.1" reset_recovery="0.0"/>
  </control>
</root>
