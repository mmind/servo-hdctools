<?xml version="1.0"?>
<root>
  <control>
    <name>krill_rom_params</name>
    <doc>Get or set EEPROM paramters on Krill</doc>
    <params cmd="get" subtype="rom_params" interface="4" drv="m24c02"
            slv="0x52"></params>
    <params cmd="set" subtype="rom_params" interface="4" drv="m24c02"
            slv="0x52" input_type="str"></params>
  </control>
  <control>
    <name>krill_rom_data</name>
    <doc>Get or set EEPROM data on Krill</doc>
    <params cmd="get" subtype="data" interface="4" drv="m24c02"
            slv="0x52"></params>
    <params cmd="set" subtype="data" interface="4" drv="m24c02"
            slv="0x52" input_type="str"></params>
  </control>
</root>
