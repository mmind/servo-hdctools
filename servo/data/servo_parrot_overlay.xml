<root>
  <!-- A DUT with non-Chrome-EC requires the 50-pin servoflex config. -->
  <include>
    <name>servoflex_v2_r0_p50.xml</name>
  </include>
  <!-- ******** Controls ******** -->
  <control>
    <name>rec_mode</name>
    <doc>Google recovery mode</doc>
    <params subtype="rec_mode" interface="7" drv="parrot_ec"
    map="onoff" clobber_ok=""></params>
  </control>
</root>
