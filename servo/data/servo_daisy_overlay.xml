<root>
  <include>
    <name>arm_ec_common.xml</name>
  </include>
  <!-- ******** Controls ******** -->
  <control>
    <name>lid_open</name>
    <params subtype="lid_open" interface="7" drv="daisy_ec" map="yesno"
            clobber_ok=""></params>
  </control>
  <control>
    <name>power_state</name>
    <doc>Used to turn the DUT off and on</doc>
    <params clobber_ok="" cmd="set" interface="servo" drv="daisy_power"
            reset_recovery="0.4" input_type="str"/>
  </control>
</root>
