<?xml version="1.0"?>
<root>
  <control>
    <name>sflex_v1_gpio_p4</name>
    <doc>pca9500_gpio_p4</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="0" map="onoff_i"></params>
  </control>
  <control>
    <name>sflex_v1_gpio_p5</name>
    <doc>pca9500_gpio_p5</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="1" map="onoff_i"></params>
  </control>
  <control>
    <name>rx_en</name>
    <doc>Enable UART RX. Must swing i2c_mux:rem</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="2" map="onoff_i"></params>
  </control>
  <control>
    <name>tx_en</name>
    <doc>Enable UART TX. Must swing i2c_mux:rem</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="3" map="onoff_i"></params>
  </control>
  <control>
    <name>sflex_v1_gpio_p9</name>
    <doc>pca9500_gpio_p9</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="4" map="onoff_i"></params>
  </control>
  <control>
    <name>id_eeprom_wc</name>
    <doc>GPIO that controls the write control bit of the PCA9500.
    Must assert in order to write the ID EEPROM</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="5" map="onoff_i"></params>
  </control>
  <control>
    <name>bios_en</name>
    <doc>Enable BIOS SPI EEPROM writing from servo. Must swing
    i2c_mux:rem</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="6" map="onoff_i"></params>
  </control>
  <control>
    <name>ec_en</name>
    <doc>Enable EC SPI EEPROM writing from servo. Must swing
    i2c_mux:rem</doc>
    <params interface="2" servo_v3_interface="4" drv="pca9500" subtype="gpio"
    slv="0x27" offset="7" map="onoff_i"></params>
  </control>
</root>
